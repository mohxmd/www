{
  "name": "www",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build --remote",
    "build:local": "astro build",
    "preview": "astro preview",
    "db:push": "astro db push --remote",
    "db:seed": "astro db execute db/seed.ts --remote",
    "✨": "──────────────── Hygiene ────────────────",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,jsx,tsx,astro}\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,astro,md,mdx,json}\"",
    "check-types": "astro check",
    "new": "node src/content/_scripts/new-post.ts",
    "update:deps": "bash ./update-deps.sh"
  },
  "dependencies": {
    "@alpinejs/focus": "^3.15.0",
    "@astrojs/alpinejs": "^0.4.9",
    "@astrojs/db": "^0.18.0",
    "alpinejs": "^3.15.0",
    "astro": "^5.14.1"
  },
  "devDependencies": {
    "@astrojs/mdx": "^4.3.6",
    "@astrojs/node": "^9.4.4",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/ts-plugin": "^1.10.4",
    "@astrojs/vercel": "8.2.8",
    "@iconify-json/arcticons": "^1.2.38",
    "@iconify-json/lucide": "^1.2.68",
    "@octokit/rest": "^22.0.0",
    "@shikijs/transformers": "^3.13.0",
    "@tailwindcss/vite": "4.1.14",
    "@types/alpinejs": "^3.13.11",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "astro-icon": "^1.1.5",
    "astro-pagefind": "^1.8.5",
    "astro-robots-txt": "^1.0.0",
    "astro-theme-toggle": "^0.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.37.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rehype-pretty-code": "^0.14.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "4.1.14",
    "typescript": "^5.9.3"
